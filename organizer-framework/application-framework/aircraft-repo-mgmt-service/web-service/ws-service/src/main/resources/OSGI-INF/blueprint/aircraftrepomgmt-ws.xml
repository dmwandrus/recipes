<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns:jaxws="http://cxf.apache.org/blueprint/jaxws"
           xmlns:cxf="http://cxf.apache.org/blueprint/core"
           xmlns:ifws="http://ws.aircraftrepomgmt.appframework.iframework.navair.mil/"
           xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
http://cxf.apache.org/blueprint/core http://cxf.apache.org/schemas/blueprint/core.xsd
http://cxf.apache.org/blueprint/jaxws http://cxf.apache.org/schemas/blueprint/jaxws.xsd">

    <cxf:bus>
        <cxf:features>
            <cxf:logging/>
        </cxf:features>
    </cxf:bus>
    <jaxws:endpoint id="AircraftRepoMgmt-webservice"
                    implementorClass="mil.navair.iframework.appframework.aircraftrepomgmt.ws.DefaultAircraftRepoMgmtWebService"
                    implementor="#DefaultAircraftRepoMgmt-webservice"
                    serviceName="ifws:Hello_Service"
                    endpointName="ifws:Hello_PortType"
                    address="/services/AircraftRepoMgmt">

    </jaxws:endpoint>


    <bean id="DefaultAircraftRepoMgmt-webservice" class="mil.navair.iframework.appframework.aircraftrepomgmt.ws.DefaultAircraftRepoMgmtWebService">
        <property name="bc" ref="blueprintBundleContext"/>
        <property name="service" ref="default-service"/>
    </bean>

    <reference id="default-service" interface="mil.navair.iframework.appframework.aircraftrepomgmt.api.AircraftRepoMgmtService"/>

</blueprint>
